{
  "dashboard": {
    "enabled": true,
    "port": 8080
  },
  "discord": {
    "token": "YOUR_DISCORD_BOT_TOKEN"
  },
  "sessions": {
    "default": {
      "name": "Claw",
      "description": "Default AI personal agent session",
      "provider": {
        "endpoint": "https://api.openai.com/v1",
        "apiKey": "sk-YOUR_API_KEY",
        "model": "gpt-4o",
        "embeddingModel": "text-embedding-3-small",
        "contextWindow": 128000
      },
      "workspace": "./data/sessions/default",
      "restrictToWorkspace": true,
      "allowSelfModify": false,
      "tools": {
        "exec": true,
        "web": true,
        "memory": true
      },
      "heartbeat": {
        "enabled": true,
        "interval": "30m",
        "activeHours": { "start": 8, "end": 22 }
      },
      "context": {
        "compressionThreshold": 0.8,
        "keepRecentMessages": 20
      },
      "discord": {
        "allowFrom": ["YOUR_DISCORD_USER_ID"],
        "channels": ["YOUR_DISCORD_CHANNEL_ID"]
      }
    },
    "coder": {
      "name": "CodeClaw",
      "description": "A session focused on software development",
      "provider": {
        "endpoint": "https://api.openai.com/v1",
        "apiKey": "sk-YOUR_API_KEY",
        "model": "gpt-4o",
        "embeddingModel": "text-embedding-3-small",
        "contextWindow": 128000
      },
      "workspace": "./data/sessions/coder",
      "restrictToWorkspace": true,
      "allowSelfModify": true,
      "tools": {
        "exec": true,
        "web": true,
        "memory": true
      },
      "heartbeat": {
        "enabled": false,
        "interval": "1h"
      },
      "context": {
        "compressionThreshold": 0.75,
        "keepRecentMessages": 30
      }
    }
  }
}
